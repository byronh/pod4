            <to-view-id>facelets/EditGroup.xhtml</to-view-id> 
        </navigation-case>
        <navigation-case>
            <from-outcome>Group Event</from-outcome>
            <to-view-id>facelets/GroupEvent.xhtml</to-view-id> 
        </navigation-case>
        <navigation-case>
            <from-outcome>Search People</from-outcome>
            <to-view-id>facelets/PeopleSearch.xhtml</to-view-id> 
        </navigation-case>
        <navigation-case>
