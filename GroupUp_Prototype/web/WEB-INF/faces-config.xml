<faces-config version="2.1"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">


    <navigation-rule>
        <!--<from-view-id>/UserAccount.xhtml</from-view-id>-->
        <navigation-case>
            <from-outcome>Schedule View</from-outcome>
            <to-view-id>facelets/ScheduleView.xhtml</to-view-id> 
        </navigation-case>
        <navigation-case>
            <from-outcome>Schedule Edit</from-outcome>
            <to-view-id>facelets/ScheduleEdit.xhtml</to-view-id> 
        </navigation-case>
        <navigation-case>
            <from-outcome>Group Create</from-outcome>
            <to-view-id>facelets/CreateGroup.xhtml</to-view-id> 
